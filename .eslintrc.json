{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "indent": ["error", "tab"],
    "no-tabs": 0,
    "key-spacing": 0,
    "space-in-parens": ["error", "never"],
    "space-before-function-paren": ["error", "always"],
    "curly": "off",
    "eqeqeq": "error",
    "no-mixed-spaces-and-tabs": "warn",
    "padded-blocks": "off",
    "no-multiple-empty-lines": "warn",
    "semi": ["warn", "never"],
    "no-debugger": "warn"
  }
}
